citHeader("To cite Anabel in publications please use:")

bibentry(bibtype = "Article",
  title        = "Anabel: An Online Tool for the Real-Time Kinetic Analysis of Binding Events",
  author       = c(as.person("Stefan Kraemer"),
                   as.person("Johannes Woehrle"),
                   as.person("Christin Rath"),
                   as.person("Guenter Roth")),
  journal      = "Bioinformatics and Biology Insights",
  year         = "2019",
  volume       = "13",
  pages        = "1--10",

  textVersion  =
  paste("Stefan Kraemer, Johannes Woehrle, Christin Rath and Guenter Roth (2019).",
        "Anabel: An Online Tool for the Real-Time Kinetic Analysis of Binding Events.",
        "Bioinformatics and Biology Insights, 13, 1-10.")
)
