+     print(k)
+     for (mylink in METHODSLINKS)
+     {
+ 	cat(mylink)
+ 	cat(mymethod)
+ 	hc <- hcluster(USArrests,link = mylink, method =  mymethod, nbproc=4)
+ 	print(hc)
+     }
+ }
