S3method(plot, kmfun, plot.fads.kmfun)
S3method(plot, ksfun, plot.fads.ksfun)
S3method(plot, krfun, plot.fads.krfun)
S3method(plot, mimetic)
S3method(plot, spp)
S3method(plot, swin)
S3method(plot, dval, plot.vads.dval)
S3method(plot, k12val, plot.vads.k12val)
