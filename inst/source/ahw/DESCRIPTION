Package: ahw
Type: Package
Title: Calculates Continuous Time Likelihood Ratio Weights Assuming
        Multiplicative Intensity Models and Additive Hazard Models
Version: 0.1.0
Authors@R: 
    c(
        person(
            given = "Kjetil",
